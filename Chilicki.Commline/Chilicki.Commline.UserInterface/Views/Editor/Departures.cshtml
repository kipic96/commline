@using Chilicki.Commline.UserInterface.Resources
@{
    ViewBag.Title = "Departures";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    

<h2>@BaseResources.Departures</h2>

<input id="time" class="timepicker" />

<section class="scripts">
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/jquery-ui-1.12.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-sliderAccess.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-timepicker-addon.min.js" type="text/javascript"></script>

    <script type="text/javascript">
        $('.timepicker').timepicker();

    </script>
</section>
